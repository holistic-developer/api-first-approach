{
  "name": "api-first",
  "version": "0.3.0",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build presentation.md --base /api-first-approach/ --out docs",
    "dev": "slidev presentation.md --open",
    "export": "slidev export presentation.md",
    "screenshot": "slidev export presentation.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.42.11",
    "codemirror-theme-vars": "^0.1.2",
    "prism-theme-vars": "^0.2.4",
    "slidev-theme-ksick-dynatrace": "^0.4.0",
    "theme-vitesse": "^0.7.2",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@iconify-json/logos": "^1.1.34",
    "@iconify-json/simple-icons": "^1.1.68",
    "@iconify-json/skill-icons": "^1.1.3",
    "@iconify-json/uim": "^1.1.6",
    "@slidev/cli": "^0.42.11"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "dark",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Poppins",
        "mono": "Fira Code"
      },
      "hightlighter": "prism"
    }
  }
}
